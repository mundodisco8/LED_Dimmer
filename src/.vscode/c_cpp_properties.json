{
    "env": {
        "SDKPath": "%HOMEPATH%/SimplicityStudio/SDKs/gecko_sdk",
        "CompilerPath": "C:/SiliconLabs/SimplicityStudio/v5/developer/toolchains/gnu_arm/10.2_2020q4/bin/"
    },
    "configurations": [
        {
            "name": "SiLabs",
            "includePath": [
                "${workspaceFolder}/**",
                "gecko_sdk_4.1.3/platform/common/toolchain/inc",
                "${workspaceFolder}/gecko_sdk_4.1.3/app/common/util/app_assert"
            ],
            "browse": {
                "path": [
                    "${SDKPath}"
                ]
            },
            "defines": [
                "BGM220PC22WGA=1",
                "SL_APP_PROPERTIES=1",
                "BOOTLOADER_APPLOADER=1",
                "SL_COMPONENT_CATALOG_PRESENT=1",
                "MBEDTLS_CONFIG_FILE=<mbedtls_config.h>",
                "MBEDTLS_PSA_CRYPTO_CONFIG_FILE=<psa_crypto_config.h>",
                "SL_RAIL_LIB_MULTIPROTOCOL_SUPPORT=0",
                "SL_RAIL_UTIL_PA_CONFIG_HEADER=<sl_rail_util_pa_config.h>",
                "SLI_RADIOAES_REQUIRES_MASKING=1"
            ],
            "compilerPath": "${CompilerPath}/arm-none-eabi-gcc.exe",
            "compilerArgs": [
                "-g3",
                "-gdwarf-2",
                "-mcpu=cortex-m33",
                "-mthumb",
                "-std=c11",
                "-Os",
                "-Wall",
                "-Wextra",
                "-ffunction-sections",
                "-fdata-sections",
                "-imacrossl_gcc_preinclude.h",
                "-mfpu=fpv5-sp-d16",
                "-mfloat-abi=hard",
                "-mcmse",
                "-c",
                "-fmessage-length=0",
                "-MMD",
                "-MP"
            ],
            "cStandard": "c11",
            "cppStandard": "c++03",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}